    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dynamic-base": "^1.2.0",
    "vite-plugin-solid": "^2.11.10",
    "vite-plugin-static-copy": "^3.1.4"
  },
